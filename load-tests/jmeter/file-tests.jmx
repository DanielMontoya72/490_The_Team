          <elementProp name="TEST_FILE_PATH" elementType="Argument">
            <stringProp name="Argument.name">TEST_FILE_PATH</stringProp>
            <stringProp name="Argument.value">${__P(test.file.path,load-tests/jmeter/data/files/)}</stringProp>
            <stringProp name="Argument.metadata">=</stringProp>
          </elementProp>
        </collectionProp>
      </elementProp>
    </TestPlan>
    <hashTree>
      <!-- HTTP Header Manager -->
      <HeaderManager guiclass="HeaderPanel" testclass="HeaderManager" testname="Storage Headers" enabled="true">
        <collectionProp name="HeaderManager.headers">
          <elementProp name="apikey" elementType="Header">
